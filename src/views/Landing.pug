nav.navbar.navbar-expand-lg.navbar-dark.navbar-custom.fixed-top
  .container
    a.navbar-brand.logo-text.page-scroll(href="/")
      h1 FETA
    button.navbar-toggler(type="button" data-bs-toggle="collapse" data-bs-target="#navbar" aria-controls="navbar" aria-expanded="false" aria-label="Toggle navigation")
      span.navbar-toggler-icon
    #navbar.collapse.navbar-collapse
      ul.navbar-nav.ms-auto
        li.nav-item
          a.btn-outline-sm(href='/login') LOG IN
        li.nav-item
          a.btn-outline-sm(href='/registration') SIGN UP

header#header.header
  .header-content
    .container
      .row
        .col-lg-6.col-xl-5
          .text-container
            h1
              | Financial Excellence Tracking Application
            p.p-large FETA will help your every day budgeting &amp; statistics needs!
            a.btn-solid-lg.page-scroll(href='/registration') SIGN UP
        .col-lg-6.col-xl-7
          .image-container
            .img-wrapper
              img.img-fluid(src='images/header-software-app.png' alt='alternative')
