  meta(charset="UTF-8")
  meta(name="viewport" content="width=device-width, initial-scale=1.0")

  meta(name='description' content='FETA is financial excellence. Track your assets with FETA.')
  meta(property='og:title' content='FETA: Financial Excellence Tracking Application')
